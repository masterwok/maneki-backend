
selectAllUsers:
SELECT *
FROM user;

insertUser:
INSERT INTO user
(
  email,
  password,
  first_name,
  last_name
)
VALUES (
  :email,
  :password,
  :firstName,
  :lastName
);

selectLastInsertedRowId:
SELECT last_insert_id();

users:
SELECT *
FROM user;

selectById:
SELECT *
FROM user
WHERE id = ?;
